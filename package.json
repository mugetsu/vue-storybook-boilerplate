{
  "name": "vue-storybook-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "storybook": "start-storybook -p 9010",
    "build": "build-storybook",
    "test": "jest",
    "storyshots": "rimraf ./__image_snapshots__ && build-storybook && jest"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@storybook/addon-a11y": "5.3.0-alpha.40",
    "@storybook/addon-actions": "5.3.0-alpha.40",
    "@storybook/addon-backgrounds": "5.3.0-alpha.40",
    "@storybook/addon-centered": "5.3.0-alpha.40",
    "@storybook/addon-docs": "5.3.0-alpha.40",
    "@storybook/addon-knobs": "5.3.0-alpha.40",
    "@storybook/addon-links": "5.3.0-alpha.40",
    "@storybook/addon-storyshots": "^5.2.5",
    "@storybook/addon-storyshots-puppeteer": "^5.2.5",
    "@storybook/addon-viewport": "5.3.0-alpha.40",
    "@storybook/vue": "5.3.0-alpha.40",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.6.1",
    "babel-preset-vue": "^2.0.2",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.13.0",
    "require-context.macro": "^1.2.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "jest": {
    "cacheDirectory": ".cache/jest",
    "clearMocks": true,
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "components/(.*)$": "<rootDir>/src/components/$1",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "node"
    ],
    "transform": {
      "^.+\\.stories\\.jsx?$": "<rootDir>/node_modules/@storybook/addon-storyshots/injectFileName",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "testPathIgnorePatterns" : [
      "<rootDir>/storyshots.test.js" 
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mugetsu/vue-storybook-boilerplate.git"
  },
  "author": {
    "name": "Randell Quitain",
    "url": "https://github.com/mugetsu"
  },
  "license": "MIT"
}
